<h1>Edit the team below:</h1>

<%= form_for(@team) do |f| %>

  <div class="field">
    <%= f.label :rank %><br>
    <%= f.text_field :rank %>
  </div>
  <div class="field">
    <%= f.label :team %><br>
    <%= f.text_field :team %>
  </div>

  <div class="field">
    <%= f.label :manager %><br>
    <%= f.text_area :manager %>
  </div>

  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

<a href="/teams">Cancel</a>